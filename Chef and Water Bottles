import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
			int a=sc.nextInt();
            int t=0;
			while(t<a){
		   int n =sc.nextInt();
		   int x=sc.nextInt();
		   int k=sc.nextInt();
		   int d=k/x;
		   if(d>n)
		   System.out.println(n);
		   else
		   System.out.println(d);
		 t++;
			}
	}
}
