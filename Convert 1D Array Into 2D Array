class Solution {
    public int[][] construct2DArray(int[] original, int m, int n) {
        int[][] newm=new int [m][n];
        int p=0;
        if(original.length==m*n){
        for(int i=0;i<m;i++){
            for(int j=0;j<n;j++){
                newm[i][j]=original[p];
                p++;
            }
        }
            return newm;
        }
        else{
            return new int[0][0];
        }
    }
}
