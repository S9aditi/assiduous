class Solution {
    public boolean checkIfExist(int[] arr) {
        for(int i=0;i<arr.length;i++){
           int n=arr[i];
            int j=arr.length-1;
            while(j>=0){
                if(i!=j&&n==2*arr[j])
                    return true;
                j--;
            }
                }
        return false;
    }
}
